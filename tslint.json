{
  "extends": "tslint:latest",
  "rules": {
    "array-type": [
      false
    ],
    "space-before-function-paren": false,
    "arrow-parens": false,
    "interface-over-type-literal": [
      false
    ],
    "max-classes-per-file": [
      4
    ],
    "interface-name": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "align": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "quotemark": [
      "single"
    ],
    "no-unused-variable": [
      true
    ]
  }
}